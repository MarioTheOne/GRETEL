{
    "parameters": {
        "lrs": {
            "generator": 0.001,
            "discriminator": 0.01
        },
        "proto_model": {
            "class": "src.explainer.generative.gans.model.GAN",
            "parameters": {
                "epochs": 100,
                "batch_size": 8,
                "generator": {
                    "class": "src.explainer.generative.gans.generators.residual_generator.ResGenerator",
                    "parameters": {
                        "node_features": 7
                    }
                },
                "discriminator": {
                    "class": "src.explainer.generative.gans.discriminators.simple_discriminator.SimpleDiscriminator",
                    "parameters": {
                        "n_nodes": 200,
                        "node_features": 7
                    }
                },
                "optimizer": {
                    "class": "torch.optim.Adam",
                    "parameters": {
                        "lr": 0.001,
                        "betas": [
                            0.9,
                            0.999
                        ],
                        "eps": 1e-08,
                        "weight_decay": 0,
                        "amsgrad": false,
                        "maximize": false,
                        "capturable": false,
                        "differentiable": false
                    }
                },
                "loss_fn": {
                    "class": "torch.nn.CrossEntropyLoss",
                    "parameters": {
                        "ignore_index": -100,
                        "reduction": "mean",
                        "label_smoothing": 0.0
                    }
                }
            }
        }
    }
}