{
    "class": "src.oracle.nn.torch.OracleTorch",
    "parameters": {
        "epochs": 200,
        "batch_size": 8,
        "optimizer": {
          "class": "torch.optim.Adam",
          "parameters": {
            "lr": 0.001,
            "betas": [
              0.9,
              0.999
            ],
            "eps": 1e-08,
            "weight_decay": 0,
            "amsgrad": false,
            "maximize": false,
            "capturable": false,
            "differentiable": false
          }
        },
        "loss_fn": {
          "class": "torch.nn.CrossEntropyLoss",
          "parameters": {     
            "reduction": "mean"
          }
        },
        "converter": {
          "class": "src.dataset.converters.tree_cycles_converter.TreeCyclesConverter",
          "parameters": {
            "feature_dim": 8
          }
        },
        "model": { 
          "class": "src.oracle.nn.gcn.GCN",
          "parameters": { "node_features": 8 }
      } 
    }   
}