{  
    "experiment" : {
        "scope": "gcn_oracle",
        "parameters" : {
            "lock_release_tout":120,
            "propagate":[
                {"in_sections" : ["explainers"],"params" : {"fold_id": 0}},
                {"in_sections" : ["oracles"],"params" : {"fold_id": -1,"retrain":false}},
                {"in_sections": ["datasets"],"params": { "compose_mes" : "config/snippets/centr_and_weights.json" }}
            ]
        }
    },
    "compose_mes" : "config/snippets/default_metrics.json",
    "compose_strs" : "config/snippets/default_store_paths.json",
    "datasets": [
        {
            "class": "src.n_dataset.dataset_base.Dataset",
            "parameters": {
                "generator": {
                    "class": "src.n_dataset.generators.asd.ASDGenerator", 
                    "parameters": { "data_dir": "data/datasets/autism/" }
                }
            } 
        } 
    ],
    "oracles": [
        {"compose_gcn" : "config/snippets/GCNOracle.json"},
        {"compose_gcn" : "config/snippets/KNNOracle.json"},
        {"compose_gcn" : "config/snippets/SVMOracle.json"}
    ],
    "explainers": [
            {
                "name": "graph_countergan", 
                "parameters": {
                    "n_nodes": 28,
                    "converter": "tree_cycles",
                    "n_labels": 2,
                    "training_iterations": 100,
                    "sampling_iterations": 50,
                    "feature_dim": 8,
                    "lr_generator": 0.001,
                    "lr_discriminator": 0.001
                } 
            }
        ]    
}