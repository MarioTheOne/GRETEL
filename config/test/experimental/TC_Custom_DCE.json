{  
    "experiment" : {
        "scope": "gcn_oracle",
        "parameters" : {
            "lock_release_tout":120,
            "propagate":[
                {"in_sections" : ["explainers"],"params" : {"fold_id": 0}},
                {"in_sections" : ["oracles"],"params" : {"fold_id": -1,"retrain":true}},
                {"in_sections": ["datasets"],"params": { "compose_mes" : "config/snippets/centr_and_weights.json" }}
            ]
        }
    },
    "compose_mes" : "config/snippets/default_metrics.json",
    "compose_strs" : "config/snippets/default_store_paths.json",
    "datasets": [
        {
            "class": "src.n_dataset.dataset_base.Dataset",
            "parameters": {
                "generator": {
                    "class": "src.n_dataset.generators.treecycles.TreeCycles", 
                    "parameters": { "num_instances": 100, "num_nodes_per_instance": 28, "num_nodes_in_cycles": 4 }
                }
            } 
        }
    ],
    "oracles": [
        {
            "class": "src.oracle.nn.torch.OracleTorch",
            "parameters": {
                "epochs": 100,
                "batch_size": 4,
                "compose_opt":"config/snippets/torch.optim/SGD.json",
                "compose_loss":"config/snippets/torch.loss/CrossEntropy.json",
                "model": { 
                  "class": "src.oracle.nn.gcn.DownstreamGCN",
                  "parameters": {"num_conv_layers":1,"num_dense_layers":2,"conv_booster":2,"linear_decay":1.8}
              } 
            }   
        }
    ],
    "explainers": [{"class": "src.explainer.search.dce.DCESearchExplainer","parameters":{}}]
}