{  
    "experiment" : {
        "scope": "tree_cycles_custom_oracle",
        "parameters" : {
            "lock_release_tout":120,
            "propagate":[
                {"in_sections" : ["explainers"],"params" : {"fold_id": 0, "retrain": true}},
                {"in_sections" : ["do-pairs/oracle"],"params" : {"fold_id": -1,"retrain": true}},
                {"in_sections": ["do-pairs/dataset"],"params": { "manipulators" : 
                [{ "class": "src.n_dataset.manipulators.causality.Causality", "parameters": {} },
                { "class": "src.n_dataset.manipulators.centralities.NodeCentrality", "parameters": {} },
                { "class": "src.n_dataset.manipulators.weights.EdgeWeights", "parameters": {} }] }}
            ]
        }
    },
    "do-pairs":[ {
        "dataset": { 
                "class": "src.n_dataset.dataset_base.Dataset",
                "parameters": {
                    "generator": {
                        "class": "src.n_dataset.generators.treecycles_rand.TreeCyclesRand", 
                        "parameters": { "num_instances": 500, "num_nodes_per_instance": 28, "ratio_nodes_in_cycles": 0.3}
                    }
                }
            },
        "oracle": {
          "class": "src.oracle.custom.oracle_tree_cycles.TreeCyclesOracle",
          "parameters": {} 
          }   
      }
    ],
    
    "explainers": [{"class": "src.explainer.generative.clear.CLEARExplainer","parameters":{ "epochs": 600, "lr": 0.01, "lambda_cfe": 0.1, "alpha": 0.4, "batch_size_ratio": 0.15 }}],
    "compose_mes" : "config/snippets/default_metrics.json",
    "compose_strs" : "config/snippets/default_store_paths.json"
}