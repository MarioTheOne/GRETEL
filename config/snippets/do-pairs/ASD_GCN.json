{
    "dataset" : {"compose_gcn" : "config/snippets/datasets/ASD.json"},
    "oracle": {
        "class": "src.oracle.nn.torch.OracleTorch",
        "parameters": {
            "epochs": 1000,
            "batch_size": 64,
            "optimizer": {
                "class": "torch.optim.RMSprop",
                "parameters": {
                    "lr":0.0000001              
                }
                },
            "loss_fn": {
                "class": "torch.nn.CrossEntropyLoss",
                "parameters": {     
                    "reduction":"mean"
                }
            },
            "model": { 
                "class": "src.oracle.nn.gcn.DownstreamGCN",
                "parameters": {"num_conv_layers":1,"num_dense_layers":1,"conv_booster":2,"linear_decay":2}
            } 
        }
    }
}