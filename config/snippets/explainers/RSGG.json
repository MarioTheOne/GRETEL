{
    "class": "src.oracle.nn.torch.OracleTorch",
    "parameters": {
        "epochs": 500,
        "batch_size": 8,
        "optimizer": {
          "class": "torch.optim.Adam",
          "parameters": {
            "lr": 0.0001,
            "betas": [
              0.9,
              0.999
            ],
            "eps": 1e-08,
            "weight_decay": 0,
            "amsgrad": false,
            "maximize": false,
            "capturable": false,
            "differentiable": false
          }
        },
        "loss_fn": {
          "class": "torch.nn.BCELoss",
          "parameters": {     
            "size_average":true, "reduce":true, "reduction":"mean"
          }
        },
        "model": { 
          "class": "src.oracle.nn.gcn.GCN",
          "parameters": {"num_conv_layers":3}
      } 
    }   
}