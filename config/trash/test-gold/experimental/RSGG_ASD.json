{  
    "compose_stores" : "config/snippets/default_store_paths.json",
    "experiment" : {
        "scope": "gcn_oracle",
        "parameters" : {
            "lock_release_tout":120,
            "propagate":[
                {"in_sections": ["datasets"],   "params": {"compose_mes" : "config/snippets/centr_and_weights.json" }},
                {"in_sections": ["oracles"],    "params": {"fold_id":  -1, "retrain": true }},
                {"in_sections": ["explainers"], "params": {"fold_id":  0, "retrain": false }}
            ]
        }
    },    
    "datasets": [{"compose_gcn" : "config/snippets/datasets/ASD.json"}],
    "oracles": [{"compose_gcn" : "config/snippets/GCNOracle.json"}],
    "explainers": [{"class": "src.explainer.generative.rsgg.RSGG","parameters":{}}],
    "compose_mes" : "config/snippets/default_metrics.json"  
}