    "version": "0.2.0",
    "configurations": [
        {
            "name": "TCR_KNN-G2V",
            "type": "python",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": ["config/test/do-pairs/TCR_KNN-G2V_DCE.jsonc", "1"],
        },
        {
            "name": "TC_Custom_DCE",
            "type": "python",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": ["config/test/experimental/TC_Custom_DCE.json", "1"],
        },
        {
            "name": "Python: Current File with profiler",
            "type": "python",
            "request": "launch",
            "module": "cProfile",
            "console": "integratedTerminal",
            "args": [
                "-o", "output/tmp.prof", "${file}","config/test/test_TC.json", "1"
            ]
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": ["config/test/test_TC.json", "1"],
        },
        {
            "name": "RSGG_ASD",
            "type": "python",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": ["config/test/experimental/RSGG_ASD.json", "1"],
        }       
    ]
