    "version": "0.2.0",
    "configurations": [

        
        {
            "name": "Python: Current File with profiler",
            "type": "python",
            "request": "launch",
            "module": "cProfile",
            "console": "integratedTerminal",
            "args": [
                "-o", "output/tmp.prof", "${file}","config/test/test_TC.json", "1"
            ]
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": ["config/test/test_TC.json", "1"],
        }
    ]
}